#include <iostream>
using namespace std;

int main()
{
    int sum1=0,sum2=0, i,A[7] = { 2,-1,55,44,-65,-100,44 };
    for (i = 0;i < 7;i++) {
        if (A[i] < 0) {
            sum1 = sum1 + 1;
        }
        else if (A[i] > 0) {
            sum2 = sum2 + 1;
        }

    }
    cout << "no of +ve terms " << sum2 << endl;
    cout << " no of -ve terms " << sum1 << endl;


        return 0;
}
